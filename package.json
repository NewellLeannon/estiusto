{
  "name": "estiusto",
  "version": "1.0.0",
  "description": "web3.js as a collection of stores for Svelte, Sapper or SvelteKit.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NewellLeannon/estiusto.git"
  },
  "author": "JaclynWaters",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "src/stores.js",
  "types": "dist/svelte-web3.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "main": "./dist/index.js",
      "module": "./dist/index.mjs",
      "svelte": "./src/stores.js",
      "types": "./dist/svelte-web3.d.ts"
    },
    "./components": {
      "import": "./src/components/index.js",
      "svelte": "./src/components/index.js"
    }
  },
  "scripts": {
    "update-chains": "node ./scripts/update-chains.js",
    "build": "rollup -c"
  },
  "dependencies": {
    "svelte": "^3.0.0",
    "svelte-proxied-store": "^0.1.2"
  },
  "devDependencies": {
    "eslint-plugin-svelte3": "^3.2.1",
    "release-it": "^14.11.8",
    "rollup": "^2.61.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.44.3",
    "web3": "1.6.1"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sveltekit",
    "svelte",
    "iste",
    "nostrum",
    "web3",
    "store"
  ],
  "files": [
    "src",
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/NewellLeannon/estiusto.git/issues"
  }
}